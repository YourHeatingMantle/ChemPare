{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debugger: main2.py",
            "type": "debugpy",
            "request": "launch",
            "program": "main2.py",
            "console": "integratedTerminal",
            "args": [
                "${input:query}"
            ]
        },
        {
            "name": "Debugger: main.py",
            "type": "debugpy",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal",
            "args": [
                "${input:query}"
            ]
        },
        {
            "name": "Debugger: module-test.py",
            "type": "debugpy",
            "request": "launch",
            "program": "dev/module-test.py",
            "console": "integratedTerminal",
            "args": [
                "${input:query}",
                "${input:supplier}"
            ]
        }
    ],
    "inputs": [
    {
      "id": "query",
      "type": "promptString",
      "description": "Reagent to query for",
      "default": "borohydride"
    },
    {
        "id":"supplier",
        "type":"pickString",
        "description": "Select a supplier",
        "options": [
            "SearchFactory",
            "SupplierLaballey",
            "SupplierLabchem",
            "SupplierChemsavers",
            "SupplierOnyxmet",
            "SupplierEsDrei",
            "SupplierSynthetika",
            "SupplierTciChemicals",
            "SupplierFtfScientific",
            "SupplierLoudwolf",
            "SupplierWarchem",
            "SupplierLaboratoriumDiscounter",
        ],
        "default": "SearchFactory"
    }
  ]
}