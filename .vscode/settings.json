    "editor.formatOnSave": true,
    "python.testing.pytestEnabled": true,
    "autoDocstring.quoteStyle": "\"\"\"",
    "editor.wordWrapColumn": 120,
    "editor.wordWrap": "off",
    "python.defaultInterpreterPath": "python",
    "yaml.schemas": {
        "file:///Users/justin.hyland/.vscode/extensions/atlassian.atlascode-3.4.23/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml",
        "https://json.schemastore.org/travis.json": "file:///Users/justin.hyland/Documents/scripts/ChemPare/.travis.yml"
