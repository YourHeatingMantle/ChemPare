        "./src",
        "./src/chempare",
        "./src/chempare/suppliers",
        "./src/chempare/datatypes"
    ],
    "editor.defaultFormatter": "ms-python.pylint",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    }
